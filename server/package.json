{
  "name": "fdm-monster",
  "author": "David Zwart",
  "license": "AGPLv3",
  "version": "1.1.2",
  "description": "FDM Monster is a bulk OctoPrint manager to set up, configure and monitor 3D printers. Our aim is to provide extremely optimized websocket performance and reliability.",
  "main": "index.js",
  "scripts": {
    "prestart": "npm ci --production",
    "start": "pm2 flush && pm2 start index.js --name FDM -e ./logs/pm2.error.log -o ./logs/pm2.out.log --time --restart-delay=1000 --update-env --exp-backoff-restart-delay=1500",
    "restart": "pm2 restart FDM",
    "pupdate": "pm2 update",
    "stop": "pm2 stop FDM",
    "stop:delete": "pm2 delete FDM",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status",
    "prettier": "prettier --write **/*.js",
    "dev": "nodemon index.js",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fdm-monster/fdm-monster"
  },
  "bin": {
    "fdm-monster": "./index.js"
  },
  "keywords": [
    "fdm-monster",
    "fdm-connector",
    "octoprint",
    "fdm",
    "3d printing",
    "pi",
    "node",
    "nestjs",
    "vue"
  ],
  "dependencies": {
    "@fdm-monster/client": "0.3.3",
    "awilix": "^6.1.0",
    "awilix-express": "6.0.0",
    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eventemitter2": "^6.4.5",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "got": "^11.8.3",
    "helmet": "^5.0.2",
    "influx": "^5.9.3",
    "is-docker": "^2.2.1",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "migrate-mongo": "^8.2.3",
    "mongoose": "^6.2.4",
    "multer": "^1.4.4",
    "node-input-validator": "^4.5.0",
    "node-upnp-ssdp": "^0.1.1",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "rxjs": "^7.5.4",
    "rxjs-websockets": "^9.0.0",
    "semver": "^7.3.5",
    "simple-git": "^3.2.6",
    "systeminformation": "^5.11.5",
    "toad-scheduler": "^1.6.0",
    "winston": "^3.6.0",
    "ws": "^8.5.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "eslint": "8.10.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-ejs-js": "0.1.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eventsource": "2.0.0",
    "express-list-endpoints": "6.0.0",
    "jest": "27.5.1",
    "jest-util": "27.5.1",
    "mongodb-memory-server": "8.4.0",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.2"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.10.0"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      ".github/*",
      ".docs/*",
      "./coverage/*",
      "file-uploads",
      "logs/*",
      "test/*",
      "images/*",
      "views/*",
      "docker/*",
      "client-vue/*"
    ],
    "delay": 2000
  }
}
