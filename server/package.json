{
  "name": "fdm-monster",
  "author": "David Zwart",
  "license": "AGPL-3.0-or-later",
  "version": "1.2.0-rc5",
  "description": "FDM Monster is a bulk OctoPrint manager to set up, configure and monitor 3D printers. Our aim is to provide extremely optimized websocket performance and reliability.",
  "main": "index.js",
  "scripts": {
    "prestart": "npm ci --production",
    "start": "pm2 flush && pm2 start index.js --name FDM -e ./logs/pm2.error.log -o ./logs/pm2.out.log --time --restart-delay=1000 --update-env --exp-backoff-restart-delay=1500",
    "restart": "pm2 restart FDM",
    "pupdate": "pm2 update",
    "stop": "pm2 stop FDM",
    "stop:delete": "pm2 delete FDM",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "dev": "nodemon index.js",
    "test": "jest --forceExit",
    "cov:show": "./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fdm-monster/fdm-monster"
  },
  "bin": {
    "fdm-monster": "./index.js"
  },
  "keywords": [
    "fdm-monster",
    "fdm-connector",
    "octoprint",
    "fdm",
    "3d printing",
    "pi",
    "node",
    "nestjs",
    "vue"
  ],
  "dependencies": {
    "@fdm-monster/client": "0.3.38",
    "awilix": "8.0.0",
    "awilix-express": "7.0.0",
    "axios": "1.1.3",
    "bcryptjs": "2.4.3",
    "cache-manager": "4.1.0",
    "connect-history-api-fallback": "2.0.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "eventemitter2": "6.4.9",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "form-data": "4.0.0",
    "got": "11.8.5",
    "helmet": "6.0.0",
    "influx": "5.9.3",
    "lodash": "4.17.21",
    "luxon": "3.0.4",
    "migrate-mongo": "9.0.0",
    "mongoose": "6.6.5",
    "multer": "1.4.5-lts.1",
    "node-input-validator": "4.5.0",
    "node-upnp-ssdp": "0.1.1",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "rxjs": "7.5.7",
    "rxjs-websockets": "9.0.0",
    "semver": "7.3.8",
    "simple-git": "3.14.1",
    "systeminformation": "5.12.7",
    "toad-scheduler": "2.0.0",
    "winston": "3.8.2",
    "ws": "8.9.0"
  },
  "devDependencies": {
    "@lcov-viewer/cli": "1.3.0",
    "@lcov-viewer/istanbul-report": "1.3.0",
    "caniuse-lite": "1.0.30001421",
    "eslint": "8.25.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.0",
    "eventsource": "2.0.2",
    "express-list-endpoints": "6.0.0",
    "jest": "29.2.0",
    "jest-27-expect-message": "1.1.0",
    "jest-util": "29.2.0",
    "mongodb-memory-server": "8.9.3",
    "nock": "13.2.9",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "supertest": "6.3.0"
  },
  "resolutions": {
    "got": "11.8.5"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.10.0"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      ".github/*",
      ".docs/*",
      "./coverage/*",
      "file-uploads",
      "logs/*",
      "test/*",
      "images/*",
      "views/*",
      "docker/*",
      "client-vue/*"
    ],
    "delay": 2000
  }
}
