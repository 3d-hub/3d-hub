{
  "name": "3d-hub",
  "author": "David Zwart",
  "license": "AGPLv3",
  "version": "0.0.4-pre1",
  "description": "3D Hub is a bulk OctoPrint manager to set up, configure and monitor 3D printers. Our aim is to provide extremely optimized performance and reliability.",
  "main": "index.js",
  "scripts": {
    "prestart": "npm ci --production",
    "start": "pm2 flush && pm2 start index.js --name 3DHub -e ./logs/pm2.error.log -o ./logs/pm2.out.log --time --restart-delay=1000 --update-env --exp-backoff-restart-delay=1500",
    "restart": "pm2 restart 3DHub",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status",
    "pupdate": "pm2 update",
    "stop": "pm2 stop 3DHub",
    "stop:delete": "pm2 delete 3DHub",
    "prettier": "prettier --write \\\"server/**/*.js\\\" \\\"./test/**/*.js\\\" \\\"./client-vue/**/*.js\\\"",
    "dev": "nodemon ./index.js",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/3d-hub/3d-hub"
  },
  "bin": {
    "3d-hub": "./app.js"
  },
  "keywords": [
    "3d-hub",
    "3d-hub-connector",
    "octoprint",
    "3d printing",
    "pi",
    "node",
    "vue"
  ],
  "dependencies": {
    "@3d-hub/client": "0.1.12",
    "awilix": "^6.0.0",
    "awilix-express": "6.0.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eventemitter2": "^6.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "got": "^11.8.2",
    "helmet": "^4.6.0",
    "influx": "^5.9.2",
    "is-docker": "^2.2.1",
    "lodash": "^4.17.21",
    "lookpath": "^1.2.2",
    "luxon": "^2.0.2",
    "migrate-mongo": "^8.2.3",
    "mongoose": "^6.0.12",
    "multer": "^1.4.3",
    "node-input-validator": "^4.4.1",
    "node-upnp-ssdp": "^0.1.1",
    "nodemon": "^2.0.13",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.1",
    "rxjs": "^7.4.0",
    "rxjs-websockets": "^9.0.0",
    "semver": "^7.3.5",
    "simple-git": "^2.46.0",
    "systeminformation": "^5.9.9",
    "toad-scheduler": "^1.6.0",
    "winston": "^3.3.3",
    "ws": "^8.2.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-ejs-js": "^0.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eventsource": "^1.1.0",
    "express-list-endpoints": "^6.0.0",
    "jest": "^27.2.4",
    "jest-util": "^27.2.4",
    "mongodb-memory-server": "^7.4.3",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.10.0"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      ".github/*",
      ".docs/*",
      "./coverage/*",
      "file-uploads",
      "logs/*",
      "test/*",
      "images/*",
      "views/*",
      "docker/*",
      "client-vue/*"
    ],
    "delay": 2000
  }
}
