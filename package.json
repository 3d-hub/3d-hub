{
  "name": "3d-print-farm",
  "version": "0.0.1",
  "description": "3DPF is a 3D printer manager to set up, configure and monitor FDM and resin printers. Our aim is to provide extremely optimized performance and reliability.",
  "main": "app.js",
  "scripts": {
    "prestart": "npm ci --production",
    "start": "pm2 flush && pm2 start app.js --name 3DPF -e ./logs/pm2.error.log -o ./logs/pm2.out.log --time --restart-delay=1000 --update-env --exp-backoff-restart-delay=1500",
    "restart": "pm2 restart 3DPF",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status",
    "pupdate": "pm2 update",
    "stop": "pm2 stop 3DPF",
    "stop:delete": "pm2 delete 3DPF",
    "prettier": "prettier --write \\\"server/**/*.js\\\" \\\"./test/**/*.js\\\" \\\"./client-vue/**/*.js\\\"",
    "dev": "nodemon ./app.js",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidzwa/3d-print-farm"
  },
  "bin": {
    "3d-print-farm": "./app.js"
  },
  "keywords": [
    "3dprintfarm",
    "3d-print-farm",
    "3d-printer",
    "octoprint",
    "pi",
    "node",
    "vue"
  ],
  "author": "David Zwart (davidzwa)",
  "license": "AGPLv3",
  "dependencies": {
    "archiver": "^5.3.0",
    "awilix": "^4.3.4",
    "awilix-express": "^4.0.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^9.0.1",
    "eventemitter2": "^6.4.4",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "flatted": "^3.1.1",
    "form-data": "^3.0.1",
    "influx": "^5.9.0",
    "is-docker": "^2.1.1",
    "lodash": "^4.17.21",
    "lookpath": "^1.2.0",
    "luxon": "^1.26.0",
    "migrate-mongo": "^8.2.2",
    "mjpeg-decoder": "^1.0.3",
    "mongoose": "^5.12.9",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "node-input-validator": "^4.4.1",
    "node-upnp-ssdp": "^0.1.1",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.1",
    "request": "^2.88.2",
    "rxjs": "^7.2.0",
    "rxjs-websockets": "^9.0.0",
    "semver": "^6.3.0",
    "simple-git": "^2.38.0",
    "systeminformation": "^5.7.7",
    "toad-scheduler": "^1.4.0",
    "wake_on_lan": "^1.0.0",
    "winston": "^3.3.3",
    "ws": "^7.5.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-ejs-js": "^0.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eventsource": "^1.1.0",
    "express-list-endpoints": "^5.0.0",
    "jest": "^26.6.3",
    "jest-util": "^26.6.2",
    "mongodb-memory-server": "^6.9.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.10.0"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "logs/*",
      "client-vue/*"
    ],
    "delay": 2000
  }
}
