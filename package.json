{
  "name": "fdm-monster",
  "author": "David Zwart",
  "license": "AGPL-3.0-or-later",
  "version": "1.4.5",
  "description": "FDM Monster is a bulk OctoPrint manager to set up, configure and monitor 3D printers. Our aim is to provide extremely optimized websocket performance and reliability.",
  "scripts": {
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc server -w --out-dir dist",
    "watch-dev": "cross-env NODE_ENV=development nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc server -d dist",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fdm-monster/fdm-monster"
  },
  "keywords": [
    "fdm-monster",
    "fdm-connector",
    "octoprint",
    "fdm",
    "3d printing",
    "pi",
    "node",
    "vue"
  ],
  "dependencies": {
    "@fdm-monster/client": "1.3.3",
    "@influxdata/influxdb-client": "1.33.2",
    "@sentry/node": "7.69.0",
    "adm-zip": "0.5.10",
    "awilix": "8.0.1",
    "awilix-express": "8.0.0",
    "axios": "1.5.0",
    "bcryptjs": "2.4.3",
    "cache-manager": "4.1.0",
    "connect-history-api-fallback": "2.0.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "eventemitter2": "6.4.9",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "form-data": "4.0.0",
    "got": "11.8.6",
    "helmet": "7.0.0",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "luxon": "3.4.3",
    "migrate-mongo": "10.0.0",
    "mongoose": "6.12.0",
    "mqtt": "5.0.5",
    "multer": "1.4.5-lts.1",
    "node-input-validator": "4.5.1",
    "nodemon": "^3.0.1",
    "octokit": "3.1.0",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "semver": "7.5.4",
    "simple-git": "3.19.1",
    "socket.io": "4.7.2",
    "toad-scheduler": "3.0.0",
    "uuid": "9.0.1",
    "winston": "3.10.0",
    "ws": "8.14.1"
  },
  "devDependencies": {
    "@lcov-viewer/cli": "1.3.0",
    "@lcov-viewer/istanbul-report": "1.4.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.85",
    "@types/node": "^20.6.1",
    "caniuse-lite": "1.0.30001534",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "8.49.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.7.0",
    "jest-27-expect-message": "1.1.0",
    "jest-util": "29.7.0",
    "mongodb-memory-server": "8.15.1",
    "nock": "13.3.3",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "supertest": "6.3.3",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "got": "11.8.6"
  },
  "ignore-engines": false,
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 8.1.4",
    "yarn": ">= 1.22.10"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      ".github/*",
      "docs/*",
      "media",
      "test/*",
      "views/*",
      "docker/*"
    ],
    "delay": 100
  }
}
