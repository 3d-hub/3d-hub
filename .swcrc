{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": "./server",
    "paths": {
      "@/*": ["./*"]
    },
  },
  "module": {
    "type": "commonjs",
    // These are defaults.
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  }
}
