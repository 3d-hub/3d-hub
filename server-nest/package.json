{
  "name": "fdm-monster-v2",
  "author": "David Zwart",
  "version": "0.0.2",
  "description": "FDM Monster is a bulk OctoPrint manager to set up, configure and monitor 3D printers. Our aim is to provide extremely optimized websocket performance and reliability.",
  "private": true,
  "license": "AGPLv3",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:prettier": "prettier --write ./**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "1.0.0",
    "@nestjs/common": "9.2.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.0",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.2.0",
    "@nestjs/platform-ws": "9.2.0",
    "@nestjs/schedule": "2.1.0",
    "@nestjs/serve-static": "3.0.0",
    "@nestjs/swagger": "6.1.3",
    "@nestjs/terminus": "9.1.2",
    "@nestjs/typeorm": "9.0.1",
    "@nestjs/websockets": "9.2.0",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "1.4.6",
    "express-session": "1.17.3",
    "lodash": "4.17.21",
    "luxon": "3.1.0",
    "mongodb": "4.11.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "swagger-ui-express": "4.6.0",
    "typeorm": "0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.0",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@types/passport-jwt": "3.0.7",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "2.0.12",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "json-to-typescript": "1.0.1",
    "mongo-unit": "3.2.0",
    "prettier": "2.7.1",
    "supertest": "6.3.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  }
}
